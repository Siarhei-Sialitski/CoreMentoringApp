@{
    Layout = "_Layout";
}

@section Headers
{}

@RenderBody()

<div class="container ">
    <div id="carouselExampleSlidesOnly" class="carousel slide" data-ride="carousel">
        <div class="carousel-inner">
            <div class="carousel-item active">
                <img src="~/images/slides/slide_1.jpg" class="d-block w-100" alt="First slide" />
            </div>
            <div class="carousel-item">
                <img src="~/images/slides/slide_2.jpg" class="d-block w-100" alt="Second slide" />
            </div>
            <div class="carousel-item">
                <img src="~/images/slides/slide_3.jpg" class="d-block w-100" alt="Third slide" />
            </div>
            <div class="carousel-item">
                <img src="~/images/slides/slide_4.jpg" class="d-block w-100" alt="Fourth slide" />
            </div>
            <div class="carousel-item">
                <img src="~/images/slides/slide_5.jpg" class="d-block w-100" alt="Fifth slide" />
            </div>
        </div>
        <a class="carousel-control-prev" href="#carouselExampleSlidesOnly" role="button" data-slide="prev">
            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
            <span class="sr-only">Previous</span>
        </a>
        <a class="carousel-control-next" href="#carouselExampleSlidesOnly" role="button" data-slide="next">
            <span class="carousel-control-next-icon" aria-hidden="true"></span>
            <span class="sr-only">Next</span>
        </a>
    </div>
</div>

@section Footers
{}